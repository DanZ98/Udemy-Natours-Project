    "url": "https://github.com/DanZ98/Udemy-Natours-Project/issues"
  },
  "homepage": "https://github.com/DanZ98/Udemy-Natours-Project#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1"
  }
}
